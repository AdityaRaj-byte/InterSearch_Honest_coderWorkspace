#include<iostream>
using namespace std;
int search_proper_index (vector<int>& nums, int target) {
        int n=nums.size();
        int k=0;
        for(int i=0;i<n;i++){
            if(nums[i]==target)
                return i;
            if(nums[i]<target)
                k=i+1;
            }
        return k;
        }
    };


	
