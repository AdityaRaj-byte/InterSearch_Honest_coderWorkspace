#include<stdio.h>
#include<stdlib.h>
void main()
{
    int k,n;
    printf("Enter number of elements of the array: ");
    scanf("%d",&n);
    int arr[n];
    printf("\nEnter the n elements: \n");
    for(int i=0;i<n;i++)
        scanf("%d",&arr[i]);
        for(int i=0;i<=(n/2);i++){
            k=arr[i];
            arr[i]=arr[n-1-i];
            arr[n-1-i]=k;}
            printf("\nUr reversed array is: ");
            for(int i=0;i<n;i++){
                printf("%d   ",arr[i]);
            }
            
        }