#include<stdio.h>
#include<stdlib.h>
int main()
{
    int n,sum=0;
    printf("Enter number of elements of the array: ");
    scanf("%d",&n);
    int arr[n];
    printf("\nEnter the n elements: \n");
    for(int i=0;i<n;i++)
        scanf("%d",&arr[i]);
        for(int i=0;i<n;i++)
        sum+=arr[i];
        printf("\n The sum of elements = %d ",sum);
        return 0;
}